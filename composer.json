{
    "name": "vzina/wechat",
    "description": "微信SDK",
    "keywords": [
        "wechat",
        "weixin",
        "weixin-sdk",
        "sdk"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "vzina",
            "email": "yeweijian299@163.com"
        }
    ],
    "require": {
        "php": ">=5.6",
        "ext-fileinfo": "*",
        "ext-openssl": "*",
        "ext-simplexml": "*",
        "guzzlehttp/guzzle": "^6.2",
        "monolog/monolog": "^1.24",
        "overtrue/socialite": "~1.3",
        "pimple/pimple": "^3.0",
        "symfony/cache": "^3.3",
        "symfony/http-foundation": "^2.7 || ^3.0",
        "symfony/psr-http-message-bridge": "^0.3 || ^1.0"
    },
    "require-dev": {
        "doctrine/instantiator": "1.0.5",
        "mikey179/vfsStream": "^1.6",
        "mockery/mockery": "~1.0",
        "overtrue/phplint": "0.2.4",
        "phpunit/phpunit": "~5.0"
    },
    "autoload": {
        "psr-4": {
            "EasyWeChat\\": "src/"
        },
        "files": [
            "src/Kernel/Support/Helpers.php",
            "src/Kernel/Helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "EasyWeChat\\Tests\\": "tests/"
        }
