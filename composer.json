{
    "name": "vzina/wechat",
    "description": "微信SDK",
    "keywords": [
        "wechat",
        "weixin",
        "weixin-sdk",
        "sdk"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "vzina",
            "email": "yeweijian299@163.com"
        }
    ],
    "require": {
        "php": ">=7.2",
        "ext-curl": "*",
        "ext-fileinfo": "*",
        "ext-openssl": "*",
        "ext-simplexml": "*",
        "guzzlehttp/guzzle": "^6.3 || ^7.0",
        "monolog/monolog": "^2.7 || ^3.1",
        "overtrue/socialite": "~2.0",
        "pimple/pimple": "^3.0",
        "psr/simple-cache": "^1.0",
        "symfony/cache": "^3.3 || ^4.3 || ^5.0",
        "symfony/event-dispatcher": "^4.3 || ^5.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.15",
        "mockery/mockery": "^1.2.3",
        "phpstan/phpstan": "^0.12.0",
        "phpunit/phpunit": "~7.5"
    },
    "autoload": {
        "psr-4": {
            "EasyWeChat\\": "src/"
        },
        "files": [
            "src/Kernel/Support/Helpers.php",
            "src/Kernel/Helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "EasyWeChat\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
          "dev-master": "2.0.x-dev"
        }
    }
}
